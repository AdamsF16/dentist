<!DOCTYPE html>
<html lang="pl">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>YourDent</title>
    <!-- @include links -->
</head>
<body>
    <!-- @include nav -->
    <main class="offer">
        <div class="wrapper">
            <section class="offer-section">
                <div class="offer-section__img offer-section__img--one" role="img" aria-label="Zdjęcie pisania w dzienniku."></div>
                <div class="offer-section__text-box">
                    <h1 class="offer-section__title">Diagnostyka</h1>
                    <ul class="offer-section__list">
                        <li class="offer-section__text">RTG</li>
                        <li class="offer-section__text">Mikrotomografia komputerowa</li>
                        <li class="offer-section__text">Tomografia komputerowa</li>
                        <li class="offer-section__text">Konsultacja lekarska</li>
                    </ul>
                </div>
            </section>
            <section class="offer-section">
                <div class="offer-section__img offer-section__img--two" role="img" aria-label="Zdjęcie aparatu ortodontycznego"></div>
                <div class="offer-section__text-box">
                    <h1 class="offer-section__title">Ortodoncja</h1>
                    <ul class="offer-section__list">
                        <li class="offer-section__text">Tradycyjne aparaty ortodontyczne</li>
                        <li class="offer-section__text">Ceramiczne aparaty ortodontyczne</li>
                        <li class="offer-section__text">Aparaty stałe samoligaturujące</li>
                        <li class="offer-section__text">Systemy nakładkowe</li>
                        <li class="offer-section__text">Aparaty ortodontyczne dla dzieci</li>
                    </ul>
                </div>
            </section>
            <section class="offer-section">
                <div class="offer-section__img offer-section__img--three" role="img" aria-label="Zdjęcie uśmiechniętego dziecka siędzacego na krzesle dentystycznym"></div>
                <div class="offer-section__text-box">
                    <h1 class="offer-section__title">Pedodoncja
                    </h1>
                    <ul class="offer-section__list">
                        <li class="offer-section__text">Lakowanie</li>
                        <li class="offer-section__text">Fluoryzacja</li>
                        <li class="offer-section__text">Światłoutwardzalne wypełnienie kompozytowe</li>
                        <li class="offer-section__text">Lapisowanie</li>
                    </ul>
                </div>
            </section>
            <section class="offer-section">
                <div class="offer-section__img offer-section__img--four" role="img" aria-label="Zdjęcie dwóch chirugów wykonujących operacje"></div>
                <div class="offer-section__text-box">
                    <h1 class="offer-section__title">Chirurgia stomatologiczna</h1>
                    <ul class="offer-section__list">
                        <li class="offer-section__text">Resekcja wierzchołka korzenia</li>
                        <li class="offer-section__text">Nacinanie/punkcja ropni wewnątrzustnych</li>
                        <li class="offer-section__text">Wydłużenie korony klinicznej</li>
                        <li class="offer-section__text">Hemisekcja</li>
                        <li class="offer-section__text">Implantologia</li>
                    </ul>
                </div>
            </section>
            <section class="offer-section">
                <div class="offer-section__img offer-section__img--five" role="img" aria-label="Zdjęcie zdrowych zębów przegryzających marchew"></div>
                <div class="offer-section__text-box">
                    <h1 class="offer-section__title">Protetyka</h1>
                    <ul class="offer-section__list">
                        <li class="offer-section__text">Korona protetyczna</li>
                        <li class="offer-section__text">Proteza szkieletowa</li>
                        <li class="offer-section__text">Proteza akrylowa</li>
                        <li class="offer-section__text">Odbudowa zęba wkładem koronowo-korzeniowym</li>
                    </ul>
                </div>
            </section>
            <section class="offer-section">
                <div class="offer-section__img offer-section__img--six" role="img" aria-label="Zdjęcie lekarzy obslugujących aparature laserową"></div>
                <div class="offer-section__text-box">
                    <h1 class="offer-section__title">Laseroterapia</h1>
                    <ul class="offer-section__list">
                        <li class="offer-section__text">Odsłanianie implantów</li>
                        <li class="offer-section__text">Wybielanie</li>
                        <li class="offer-section__text">Nacięcia</li>
                        <li class="offer-section__text">Odkażanie systemy kanałowego</li>
                    </ul>
                </div>
            </section>
        </div>
    </main>





    <!-- @include footer -->
    <script src="./dist/js/offer.min.js"></script>
</body>